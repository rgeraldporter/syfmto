{
  "name": "syfmto",
  "version": "1.0.0",
  "preferGlobal": false,
  "description": "A functional-style validation module, allowing you to aggregate multiple failures rather than throwing at the first error.",
  "main": "./app/index.js",
  "keywords": [
    "functional",
    "ramda",
    "validation",
    "error handling",
    "success",
    "failure"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rgeraldporter/syfmto.git"
  },
  "author": "Rob Porter <rob@inpictures.ca>",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/browserify/bin/cmd.js ./src/index.js -o ./dist/syfmto.js -s Validation --no-bundle-external -t [ babelify --presets [ es2015 react ] ] && babel --presets es2015 src -d app",
    "unit": "find ./app -name '*.spec.js' | ./node_modules/cross-env/bin/cross-env.js NODE_ENV=test xargs node ./node_modules/jasmine/bin/jasmine.js"
  },
  "dependencies": {
    "ramda": "0.23.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "browserify-shim": "^3.8.13",
    "cross-env": "^3.1.4",
    "jasmine": "^2.5.3"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "ramda": "global:R"
  }
}